<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="LatinCMS" namespace="LatinCMS.Models" xmlns="urn:nhibernate-mapping-2.2">
  <class name="UsuarioModel" table="Usuario" lazy="true" >
    <id name="Id" column="id">
      <generator class="identity" />
    </id>
    <property name="Rol">
      <column name="rol" sql-type="bigint" not-null="true" />
    </property>
    <property name="Nombre">
      <column name="nombre" sql-type="varchar" not-null="true" />
    </property>
    <property name="Apellido">
      <column name="apellido" sql-type="varchar" not-null="true" />
    </property>
    <property name="Mail">
      <column name="mail" sql-type="varchar" not-null="true" />
    </property>
    <property name="Password">
      <column name="pass" sql-type="varchar" not-null="true" />
    </property>
  </class>
</hibernate-mapping>